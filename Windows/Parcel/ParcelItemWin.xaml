        <local:ParcelUC DataContext="{Binding Path=VModel}" ParentDataContext="{Binding RelativeSource={RelativeSource AncestorType=Window,AncestorLevel=1,Mode=FindAncestor},Path=DataContext,Mode=OneWay}"/>
