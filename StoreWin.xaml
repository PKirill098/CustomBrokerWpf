<Window x:Class="KirillPolyanskiy.CustomBrokerWpf.StoreWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Склад" Name="winStore" Height="136" Width="188" Loaded="winStore_Loaded" Closing="Window_Closing" Icon="/CustomBrokerWpf;component/Images/forklifter.png">
    <Grid >
        <DataGrid Name="mainDataGrid" AutoGenerateColumns="False" HeadersVisibility="None" CanUserDeleteRows="False" CanUserAddRows="False">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="storeNameColumn" Binding="{Binding Path=storeName}" Width="*" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
